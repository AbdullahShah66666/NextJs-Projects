{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "settings": {
    "react": {
      "version": "19"
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn"
  }
}
